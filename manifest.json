{
  "manifest_version": 2,

  "name": "Neckbeard Nunez",
  "description": "Fixing the web to give Steven Nunez the respect he deserves.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "nunez-icon.png",
    "default_title": "Neckbeard Nunez"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery-2.1.4.min.js", "replace.js"],
      "run_at": "document_end"
    }
  ]
}